# Static environment variables for all instances
# These variables will be included in every simulator instance

# App configuration
VITE_APP_NAME=Rift
VITE_APP_VERSION=1.0.0

# Feature flags
VITE_ENABLE_LOGGING=true
VITE_DEBUG_MODE=false

# WebRTC configuration
VITE_ICE_SERVER_URLS=stun:stun.l.google.com:19302
VITE_MAX_PEERS=4
VITE_MEDIA_CONSTRAINTS={"video":true,"audio":true}

# Add any other static environment variables below
# VITE_API_KEY=your_api_key_here
# VITE_OTHER_STATIC_VAR=static_value
